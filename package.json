{
  "name": "ts-class-gen",
  "version": "0.0.8",
  "description": "Simple class generator for typescript.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/LZhenHong/ts-class-gen"
  },
  "scripts": {
    "build": "tsc",
